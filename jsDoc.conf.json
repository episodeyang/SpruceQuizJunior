{
	"tags"      : {
		"allowUnknownTags" : true
	},
	"plugins"   : ["plugins/markdown"],
	"templates" : {
		"cleverLinks"     : true,
		"monospaceLinks"  : false,
		"default"         : {
			"outputSourceFiles" : true
		},
		"systemName"      : "SpruceQuiz Server",
		"footer"          : "",
		"copyright"       : "SpruceLearning Copyright Â© 2013-2014 Ge Yang.",
		"navType"         : "vertical",
		"theme"           : "cerulean",
		"linenums"        : true,
		"collapseSymbols" : true,
		"inverseNav"      : false
	},
	"markdown"  : {
		"parser"   : "gfm",
		"hardwrap" : true
	},
    "opts": {
        "encoding": "utf8",
        "destination": "./docs/",
        "recurse": true
    }
}
